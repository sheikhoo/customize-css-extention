{
  "manifest_version": 2,
  "name": "My Custom CSS",
  "description": "Insert custom CSS into websites",
  "author": "sheikhoo.iran@gmail.com",
  "homepage_url": "https://github.com/sheikhoo/customize-css-extention",
  "version": "1.0",
  "permissions": ["activeTab", "tabs", "storage"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["js/load.js"]
    }
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  }
}
